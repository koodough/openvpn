client
<% if node["openvpn"]["type"] == "server-bridge" -%>
dev tap
client-to-client
<% else -%>
dev tun
<% end -%>
proto <%= node["openvpn"]["proto"] %>
remote <%= node["openvpn"]["gateway"] %> <%= node["openvpn"]["port"] %>
resolv-retry infinite
nobind
persist-key
persist-tun
ca ca.crt
cert <%= @username %>.crt
key <%= @username %>.key
comp-lzo
verb 3
